{
  "extends": "airbnb-base",
  "globals": {
    "window": true,
    "document": true,
    "navigator": true,
    "require": true,
    "cordova": true,
    "angular": true,
    "WebSocket": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "inject": true,
    "__CONFIG__": true,
    "nfc": true,
    "Scandit": true
  },
  "rules": {
    "no-use-before-define": [
      "error",
      "nofunc"
    ],
    "no-control-regex": 0,
    "no-useless-escape": 0,
    "no-alert": 0,
    "import/no-dynamic-require": 0,
    "no-param-reassign": 0,
    "global-require": 0
  }
}